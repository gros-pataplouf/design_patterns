{
    "compilerOptions": {
      "target": "ES2022",
      "strict": true,
      "outDir": "./built",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    },
    "include": ["./*/*.ts"]
  }